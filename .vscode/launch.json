    "version": "0.2.0",
    "configurations": [
        {
            "name": "openai_demo",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define=OPENAI_API_KEY=<YOUR-OPENAI_API_KEY>",
                "--dart-define=OPENAI_ORG_ID=<YOUR-OPENAI_ORG_ID>"
